        <% for(let k=0;k<5;k++){ %>
          <div class="col-md-8 ml-auto mr-auto">
            <% let flag
             for(flag=0;data[flag].type!=k;flag++){ 
             } %>
            <h3 class="title"><%= data[flag].subtitle %></h3>
          </div>
          <div class="row">
          <!--article set-->
          <% for(let i=0;i<data.length;i++){ %>
            <% if(data[i].type==k){ %>
              <div class="col-md-3">
                <div class="info">
                  <div class="icon icon-danger">
                    <img src="<%= data[i].picURL %>" height="200" width="250">  
                  </div>
                  <div class="description">
                    <h5 class="info-title"><%= data[i].title %></h5>
                    <p class="description"><%= data[i].subtitle %></p>
                    <a href="<%= data[i].categary %>" class="btn btn-link btn-danger">看文章</a>
                  </div>
                </div>
              </div>
            <% } %>
          <% } %>
          </div>
          <br/><br/><br/>
        <% } %>
      </div>
    </div>

